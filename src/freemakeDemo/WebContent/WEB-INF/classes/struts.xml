<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="tutorial" extends="struts-default">
       <action name="update" class="com.action.UpdateAction">
       <result type="redirect">/page/success.html</result>
       </action>
<action name="login" class="com.action.LoginAction">
       <result type="redirect">/html/${msg}</result>
       </action>
<action name="index" class="com.action.IndexAction">
       <result type="redirect">/html/index.html</result>
       </action>
<action name="module" class="com.action.ModuleAction">
       <result type="redirect">/html/index.html</result>
       </action>
    </package>
</struts>
